                            <label for="enfocado" class="form-label">Enfocado</label>
                            <input type="range" class="form-range" min="0" max="5" id="enfocado">
                            <label for="trabajoEq" class="form-label">Trabajo en Equipo</label>
                            <input type="range" class="form-range" min="0" max="5" id="trabajoEq">
                            <label for="comprometido" class="form-label">Comprometido</label>
                            <input type="range" class="form-range" min="0" max="5" id="comprometido">
                            <label for="comunicacion" class="form-label">Comunicacion</label>
                            <input type="range" class="form-range" min="0" max="5" id="comunicacion">
                            <label for="aprendizaje" class="form-label">Capacidad de aprendizaje</label>
                            <input type="range" class="form-range" min="0" max="5" id="aprendizaje">
                            <label for="resProblem" class="form-label">Resolucion de Problemas</label>
                            <input type="range" class="form-range" min="0" max="5" id="resProblem">
